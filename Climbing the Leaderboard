#include <stdio.h>

int main() {
    int n, m;
    
    // Input leaderboard size
    scanf("%d", &n);
    int ranked[n];
    for (int i = 0; i < n; i++)
        scanf("%d", &ranked[i]);
    
    // Input player scores
    scanf("%d", &m);
    int player[m];
    for (int i = 0; i < m; i++)
        scanf("%d", &player[i]);
    
    // Step 1: Remove duplicates from ranked[] to create dense ranking
    int unique[n], size = 0;
    unique[size++] = ranked[0];
    for (int i = 1; i < n; i++) {
        if (ranked[i] != ranked[i - 1]) {
            unique[size++] = ranked[i];
        }
    }
    
    // Step 2: Traverse from end (player scores are ascending)
    int index = size - 1;  // start from the lowest rank
    for (int i = 0; i < m; i++) {
        int score = player[i];
        
        // While player score >= leaderboard score, move up
        while (index >= 0 && score >= unique[index]) {
            index--;
        }
        
        // Rank is index+2 because index points to just smaller element
        printf("%d\n", index + 2);
    }
    
    return 0;
}

